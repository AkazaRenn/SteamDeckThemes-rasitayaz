{
  "name": "Transparent Library Info",
  "version": "v1.0",
  "author": "RasitAyaz",
  "manifest_version": 5,
  "target": "Library",
  "description": "Hides backgrounds of capsule info boxes in library, looks great with the SteamOS 3.5 UI.",
  "inject": {
    "shared.css": ["SP"]
  },
  "patches": {
    "All achieved medals": {
      "type": "checkbox",
      "default": "No",
      "values": {
        "No": {
          "no_all_achieved_medal.css": ["SP"]
        },
        "Yes": {}
      }
    }
  }
}
